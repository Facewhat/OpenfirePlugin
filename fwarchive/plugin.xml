					description="Click to archive_search" />
				<!-- <item id="arc_setting" name="活跃用户展示" url="archive_setting.jsp"
					description="Click to archive_setting" />
				<item id="arc_audit" name="消息审计" url="archive_audit.jsp"
					description="Click to archive_audit" />
				<item id="conversation_display" name="活跃会话展示" url="conversation_display.jsp"
					description="Click to conversation_display" />
					-->
			</sidebar>
		</tab>
	</adminconsole>
</plugin>
 