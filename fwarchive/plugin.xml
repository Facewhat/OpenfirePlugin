<?xml version="1.0" encoding="UTF-8"?>
<plugin>

	<class>com.facewhat.archive.ArchivePlugin</class>
	<name>Face What Archive</name>
	<description>XEP-0136 compliant server side message archive for
		Openfire.</description>
	<author>lxy</author>
	<version>1.0.0</version>
	<date>04/3/2017</date>
	<minServerVersion>3.4.0</minServerVersion>

	<databaseKey>fwarchive</databaseKey>
	<databaseVersion>2</databaseVersion>
	<licenseType>gpl</licenseType>

	<adminconsole>
